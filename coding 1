a, b, c, d, e = map(int, input().split())
num = [a,b,c,d,e]
num.sort()
print(num[1])

def num(a,b):
  if a<b:
    return b
    b==x
    print(x)
  else:
    return a
    a==x
    print(x)

a,b = map(int, input().split())
print(num(a,b))

if num(a,b) in range(0,10):
  print("*"*num(a,b))
else:
  print("1이상 10이하의 숫자를 입력하세요.")
